/** 视频样式 */

.bg_box {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
}

.bg_box video {
  width: 100%;
  height: 100%;
}

/** 图片样式 */

.imagesize {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}

.imagesize image {
  width: 100%;
  margin-top: 180rpx;
}

.logo-img {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}

.logo {
  width: 500rpx;
  height: 100rpx;
  margin-top: 20rpx;
}
